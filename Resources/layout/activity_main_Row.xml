<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:orientation="vertical"
        android:background="@android:color/transparent"
        android:id="@+id/activity_main_Row_lytLinhaDivisora1"
        android:layout_width="match_parent"
        android:layout_height="6dp"/>
    <LinearLayout
        android:orientation="horizontal"
        android:background="@drawable/shapebranco"
        android:id="@+id/activity_main_Row_lytCorpoMaster"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/activity_main_Row_lblTexto"
            android:text="Menu 1"
            android:layout_marginLeft="25dp"
            android:textSize="20dp"
            android:textStyle="bold"
            android:paddingVertical="25dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:background="@android:color/transparent"
        android:id="@+id/activity_main_Row_lytLinhaDivisora2"
        android:layout_width="match_parent"
        android:layout_height="6dp"/>
</LinearLayout>
