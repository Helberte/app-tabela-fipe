<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:orientation="vertical"
        android:background="@android:color/transparent"
        android:id="@+id/activity_main_Row_lytLinhaDivisora1"
        android:layout_width="match_parent"
        android:layout_height="6dp"/>
    <LinearLayout
        android:orientation="horizontal"
        android:background="@drawable/shapebranco"
        android:id="@+id/activity_main_Row_lytCorpoMaster"
        android:layout_width="match_parent"
        android:gravity="center"
        android:paddingHorizontal="20dp"
        android:paddingVertical="8dp"
        android:layout_height="70dp">
        <ImageView
            android:id="@+id/activity_main_Row_imgIconeConsulta"
            android:src="@drawable/icons8_lupa_48"
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="7dp"
            android:contentDescription="Icone consulta veiculo" />

        <LinearLayout
            android:id="@+id/activity_main_Row_lytCorpoInfo"
            android:gravity="center"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/activity_main_Row_lblTexto"
                android:text="Consultar veículo"
                android:textSize="17dp"
                android:textStyle="bold"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <TextView
                android:id="@+id/activity_main_Row_lblSubTitulo"
                android:text="Aqui você acha o preço médio do seu veiculo"
                android:textSize="13dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
        </LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:background="@android:color/transparent"
        android:id="@+id/activity_main_Row_lytLinhaDivisora2"
        android:layout_width="match_parent"
        android:layout_height="6dp"/>
</LinearLayout>
